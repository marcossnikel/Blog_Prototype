<%-include ("../../partials/header.ejs")%>
<%-include ("../../partials/navbar.ejs")%>

<div class="container">
    <hr>
    <h2>Register</h2>
    <a class="btn btn-danger" href="/admin/users/create">Register a new User </a>
    <hr>
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>ID</th>
                <th>email</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user =>{%>
             <tr>
                    <th><%=user.id%></th>
                    <th><%=user.email%></th>
            </tr>
            <% }) %>
        </tbody>
    </table>
</div>

<%-include ("../../partials/footer.ejs")%>